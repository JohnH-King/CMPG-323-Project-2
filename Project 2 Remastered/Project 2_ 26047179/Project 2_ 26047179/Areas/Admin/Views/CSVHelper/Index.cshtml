@model IEnumerable<Project_2__26047179.Models.ViewModels.CSVViewModel>
@{

}
<br />
<div class="container row p-0 m-0">
    <!-- these are all bootstrap classes-->
    <div class="col-9">
        <h2 class="text-info">CSV</h2>
    </div>

    <div>
        @if (Model.Count() > 0)
        {
            <table class="table table-striped border">
                <tr class="table-secondary">
                    <th>
                        @Html.DisplayNameFor(m => m.EmployeeNumber)
                    </th>
                    <th>
                        @Html.DisplayNameFor(m => m.Department)
                    </th>
                    <th></th>
                    <th></th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(m => item.EmployeeNumber)
                        </td>
                        <td>
                            @Html.DisplayFor(m => item.Department)
                        </td>
                        <td>
                        </td>
                        <td>
                            <partial name="_TableButtonPartial" model="item.Id" />
                        </td>
                    </tr>
                }
            </table>
        }
        else
        {
            <p>No Job Information exists...</p>
        }
    </div>


</div>