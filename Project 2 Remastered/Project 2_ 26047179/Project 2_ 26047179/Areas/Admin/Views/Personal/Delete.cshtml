@model Project_2__26047179.Models.Personal
@{
    ViewData["Title"] = "Delete";
}
<br />
<h1 class="text-info">Delete Job Information</h1>
<br />

<fieldset>
<form method="post" asp-action="Delete">
    <div class="border" style="background-color: lightcyan; padding:10px;">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Employee.EmployeeNumber" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Employee.EmployeeNumber" readonly class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Age" class="custom-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Age" class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-5 offset-2">
                <partial name="_DeleteAndBackToListButton" model="Model.Id" />
            </div>
        </div>
    </div>
</form>
</fieldset>